<?xml version="1.0"?>
<odoo>
    <record id="jsworks_temps_view_tree" model="ir.ui.view">
        <field name="name">jsworks.temps.tree</field>
        <field name="model">jsworks.temps</field>
        <field name="inherit_id" ref="jsworks.jsworks_temps_view_tree"/>
        <field name="arch" type="xml">
            <tree string="Temps">
              <field name="Taux_horaire" position="after">
                <field name="Facture" />
              </field>
            </tree>
        </field>
    </record>
    <record id="jsworks_temps_view_form" model="ir.ui.view">
        <field name="name">jsworks.temps.form</field>
        <field name="model">jsworks.temps</field>
        <field name="inherit_id" ref="jsworks.jsworks_temps_view_form"/>
        <field name="arch" type="xml">
          <field name="Taux_horaire" position="after">
            <field name="Facture" />
          </field>
        </field>
    </record>
</odoo>