<?xml version="1.0"?>
<odoo>
    <record id="jsworks_development_action" model="ir.actions.act_window">
        <field name="name">development</field>
        <field name="res_model">jsworks.development</field>
        <field name="view_mode">tree,form</field>
    </record>
    <record id="jsworks_development_view_tree" model="ir.ui.view">
        <field name="name">jsworks.development.tree</field>
        <field name="model">jsworks.development</field>
        <field name="arch" type="xml">
            <tree string="development">
              <field name="nProject" />
              <field name="nState" />
              <field name="nCreationWork" />
              <field name="nSolutionWork" />
              <field name="Description" />
              <field name="Steps" />
              <field name="Origin" />
              <field name="Solution" />
              <field name="nCategorie" />
              <field name="isBug" />
              <field name="nDemander" />
              <field name="nSheetRef" /> 
              <field name="Project" />
              <field name="State" />
              <field name="CreationWork" />
              <field name="SolutionWork" />
              <field name="Categorie" />
<!--views_class_py_one_to_many_field_line-->
            </tree>
        </field>
    </record>
    <record id="jsworks_development_view_form" model="ir.ui.view">
        <field name="name">jsworks.development.form</field>
        <field name="model">jsworks.development</field>
        <field name="arch" type="xml">
            <form string="development">
                <sheet>
                    <group>
              <field name="nProject" />
              <field name="nState" />
              <field name="nCreationWork" />
              <field name="nSolutionWork" />
              <field name="Description" />
              <field name="Steps" />
              <field name="Origin" />
              <field name="Solution" />
              <field name="nCategorie" />
              <field name="isBug" />
              <field name="nDemander" />
              <field name="nSheetRef" /> 
              <field name="Project" />
              <field name="State" />
              <field name="CreationWork" />
              <field name="SolutionWork" />
              <field name="Categorie" />
<!--views_class_py_one_to_many_field_line-->
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
