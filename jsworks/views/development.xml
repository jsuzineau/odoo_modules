<?xml version="1.0"?>
<odoo>
    <record id="a_jsworks_development" model="ir.actions.act_window">
        <field name="name">Development</field>
        <field name="res_model">jsworks.development</field>
        <field name="view_mode">tree,form</field>
    </record>
    <record id="jsworks_development_view_tree" model="ir.ui.view">
        <field name="name">jsworks.development.tree</field>
        <field name="model">jsworks.development</field>
        <field name="arch" type="xml">
            <tree string="Developments">
              <field name="Description" />
              <field name="Steps" />
              <field name="Origin" />
              <field name="Solution" />
              <field name="isBug" />
              <field name="Project" />
              <field name="State" />
              <field name="CreationWork" />
              <field name="SolutionWork" />
              <field name="Categorie" />
            </tree>
        </field>
    </record>
    <record id="jsworks_development_view_form" model="ir.ui.view">
        <field name="name">jsworks.development.form</field>
        <field name="model">jsworks.development</field>
        <field name="arch" type="xml">
            <form string="Development">
                <sheet>
                    <group>
                      <field name="Description" />
                      <field name="Steps" />
                      <field name="Origin" />
                      <field name="Solution" />
                      <field name="isBug" />
                      <field name="Project" />
                      <field name="State" />
                      <field name="CreationWork" />
                      <field name="SolutionWork" />
                      <field name="Categorie" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
