<?xml version="1.0"?>
<odoo>
    <record id="jsworks_project_action" model="ir.actions.act_window">
        <field name="name">project</field>
        <field name="res_model">jsworks.project</field>
        <field name="view_mode">tree,form</field>
    </record>
    <record id="jsworks_project_view_tree" model="ir.ui.view">
        <field name="name">jsworks.project.tree</field>
        <field name="model">jsworks.project</field>
        <field name="arch" type="xml">
            <tree string="project">
              <field name="Name" /> 
<!--views_class_py_many_to_one_field_line-->
<!--views_class_py_one_to_many_field_line-->
            </tree>
        </field>
    </record>
    <record id="jsworks_project_view_form" model="ir.ui.view">
        <field name="name">jsworks.project.form</field>
        <field name="model">jsworks.project</field>
        <field name="arch" type="xml">
            <form string="project">
                <sheet>
              <field name="Name" /> 
<!--views_class_py_many_to_one_field_line-->
<!--views_class_py_one_to_many_field_line-->
                </sheet>
            </form>
        </field>
    </record>
</odoo>
