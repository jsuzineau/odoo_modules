<?xml version="1.0"?>
<odoo>
    <record id="jsworks_tag_action" model="ir.actions.act_window">
        <field name="name">tag</field>
        <field name="res_model">jsworks.tag</field>
        <field name="view_mode">tree,form</field>
    </record>
    <record id="jsworks_tag_view_tree" model="ir.ui.view">
        <field name="name">jsworks.tag.tree</field>
        <field name="model">jsworks.tag</field>
        <field name="arch" type="xml">
            <tree string="tag">
              <field name="idType" />
              <field name="Name" /> 
<!--views_class_py_many_to_one_field_line-->
<!--views_class_py_one_to_many_field_line-->
            </tree>
        </field>
    </record>
    <record id="jsworks_tag_view_form" model="ir.ui.view">
        <field name="name">jsworks.tag.form</field>
        <field name="model">jsworks.tag</field>
        <field name="arch" type="xml">
            <form string="tag">
                <sheet>
              <field name="idType" />
              <field name="Name" /> 
<!--views_class_py_many_to_one_field_line-->
<!--views_class_py_one_to_many_field_line-->
                </sheet>
            </form>
        </field>
    </record>
</odoo>
