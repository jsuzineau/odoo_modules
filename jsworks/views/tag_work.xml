<?xml version="1.0"?>
<odoo>
    <record id="jsworks_tag_work_action" model="ir.actions.act_window">
        <field name="name">tag_work</field>
        <field name="res_model">jsworks.tag_work</field>
        <field name="view_mode">tree,form</field>
    </record>
    <record id="jsworks_tag_work_view_tree" model="ir.ui.view">
        <field name="name">jsworks.tag_work.tree</field>
        <field name="model">jsworks.tag_work</field>
        <field name="arch" type="xml">
            <tree string="tag_work">
              <field name="idTag" />
              <field name="idWork" /> 
<!--views_class_py_many_to_one_field_line-->
<!--views_class_py_one_to_many_field_line-->
            </tree>
        </field>
    </record>
    <record id="jsworks_tag_work_view_form" model="ir.ui.view">
        <field name="name">jsworks.tag_work.form</field>
        <field name="model">jsworks.tag_work</field>
        <field name="arch" type="xml">
            <form string="tag_work">
                <sheet>
              <field name="idTag" />
              <field name="idWork" /> 
<!--views_class_py_many_to_one_field_line-->
<!--views_class_py_one_to_many_field_line-->
                </sheet>
            </form>
        </field>
    </record>
</odoo>
