<?xml version="1.0"?>
<odoo>
    <record id="jsworks_tag_development_action" model="ir.actions.act_window">
        <field name="name">tag_development</field>
        <field name="res_model">jsworks.tag_development</field>
        <field name="view_mode">tree,form</field>
    </record>
    <record id="jsworks_tag_development_view_tree" model="ir.ui.view">
        <field name="name">jsworks.tag_development.tree</field>
        <field name="model">jsworks.tag_development</field>
        <field name="arch" type="xml">
            <tree string="tag_development">
              <field name="idTag" />
              <field name="idDevelopment" /> 
<!--views_class_py_many_to_one_field_line-->
<!--views_class_py_one_to_many_field_line-->
            </tree>
        </field>
    </record>
    <record id="jsworks_tag_development_view_form" model="ir.ui.view">
        <field name="name">jsworks.tag_development.form</field>
        <field name="model">jsworks.tag_development</field>
        <field name="arch" type="xml">
            <form string="tag_development">
                <sheet>
                    <group>
              <field name="idTag" />
              <field name="idDevelopment" /> 
<!--views_class_py_many_to_one_field_line-->
<!--views_class_py_one_to_many_field_line-->
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
